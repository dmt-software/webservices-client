{
    "name": "dmt-software/webservices-client",
    "description": "An implementation of the CompanyInfo WebservicesNl data integration",
    "keywords": ["WebservicesNl", "CompanyInfo", "Data Integration", "client", "SOAP"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Bas de Mes",
            "email": "bas@dmt-software.nl"
        }
    ],
    "require": {
        "php": "^7.1",
        "dmt-software/jms-soap-serializer": "^1.0",
        "dmt-software/command-bus-validator": "^1.0",
        "guzzlehttp/guzzle": "^6.3",
        "doctrine/cache": "^1.7"
    },
    "autoload": {
        "psr-4": {
            "DMT\\WebservicesNl\\Client\\": "src/"
        }
    },
    "suggest": {
        "dmt-software/webservices-dutchbusiness": "Enables the DutchBusiness service methods"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "symfony/dotenv": "^4.1"
    },
    "autoload-dev": {
        "psr-4": {
            "DMT\\Test\\WebservicesNl\\Client\\": "tests/"
        }
    }
}
